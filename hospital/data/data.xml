<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for hospital patince record requests -->
        <record id="hospital_patince_seq" model="ir.sequence">
            <field name="name">Patience Sequnce</field>
            <field name="code">hosp_patience.seq</field>
            <field name="prefix">HP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="appointment_patince_seq" model="ir.sequence">
            <field name="name">Patience Sequnce</field>
            <field name="code">appointment.seq</field>
            <field name="prefix">AP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

           <record id="doctor_seq" model="ir.sequence">
            <field name="name">Doctor Sequnce</field>
            <field name="code">doctor.seq</field>
            <field name="prefix">DOC</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
